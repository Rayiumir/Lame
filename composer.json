{
    "name": "rayium/lame",
    "description": "Authentication Mobile and Email for Laravel",
    "homepage": "https://github.com/Rayiumir/Lame",
    "minimum-stability": "stable",
    "keywords": [
        "laravel",
        "authentication",
        "mobile",
        "email",
        "mobile-verification",
        "email-verification",
        "otp",
        "2fa",
        "laravel-package",
        "laravel-authentication",
        "laravel-mobile-authentication",
        "laravel-email-authentication",
        "laravel-mobile-verification",
        "laravel-email-verification",
        "laravel-otp",
        "laravel-2fa",
        "laravel-package-authentication",
        "laravel-package-mobile-authentication",
        "laravel-package-email-authentication",
        "laravel-package-mobile-verification",
        "laravel-package-email-verification",
        "laravel-package-otp",
        "laravel-package-2fa",
        "laravel-package-authentication-mobile",
        "laravel-package-authentication-email",
        "laravel-package-authentication-mobile-verification",
        "laravel-package-authentication-email-verification",
        "laravel-package-authentication-otp",
        "laravel-package-authentication-2fa"
    ],
    "license": "MIT",
    "type": "package",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Rayium\\Lame\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rayium\\Lame\\Tests": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/Rayiumir/Lame/issues",
        "source": "https://github.com/Rayiumir/Lame"
    },
    "authors": [
        {
            "name": "Raymond Baghumian",
            "email": "rayiumir@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.21|^10.0|^11.0",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "ghasedak/laravel": "^2.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3|^10.4|^11.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
